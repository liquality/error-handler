{
  "name": "@liquality/error-parser",
  "version": "1.0.0",
  "description": "Error Handling Library for liquality",
  "repository": "git@github.com:liquality/error-parser.git",
  "author": "koderholic <meetenebelioluchi@gmail.com>",
  "license": "MIT",
  "module": "dist/index.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "dist": "dist",
    "src": "src"
  },
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "@babel/types": "^7.17.0",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/chrome": "^0.0.179",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-jest": "^26.1.1",
    "husky": "^8.0.1",
    "jest": "^27.5.1",
    "pinst": "^3.0.0",
    "prettier": "^2.5.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "scripts": {
    "build": "tsc -p src/",
    "watch": "tsc -w -p src/",
    "lint": "eslint . --ext .ts",
    "prettier:fix": "prettier --write .",
    "test": "jest --coverage",
    "test:integration": "jest --verbose --forceExit",
    "test:watch": "jest --watch",
    "test:clearCache": "jest --clearCache",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "packageManager": "yarn@3.1.1",
  "publishConfig": {
    "access": "public"
  }
}
